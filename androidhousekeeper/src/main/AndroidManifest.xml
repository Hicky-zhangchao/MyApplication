<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.feicuiedu.androidhousekeeper">

    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application android:allowBackup="true" android:icon="@drawable/housekeeper"
        android:label="@string/app_name" android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".LogoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".StartActivity"/>
        <activity android:name=".SoftwareInfoActivity"/>
        <activity android:name=".SpeedActivity"/>
        <activity android:name=".SoftwareManagementActivity"/>
        <activity android:name=".PhoneTestingActivity"/>
        <activity android:name=".CommuniCationActivity"/>
        <activity android:name=".FileManagementActivity"/>
        <activity android:name=".CCleanerActivity"/>
        <activity android:name=".SoftwaresetActivity"/>
        <activity android:name=".SoftAllActivity"/>
        <activity android:name=".SoftServerActivity"/>
        <activity android:name=".SoftApplyActivity"/>
        <activity android:name=".FileAllActivity"/>
    </application>

</manifest>
